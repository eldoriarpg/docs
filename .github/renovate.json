{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "gradle": {
    "enabled": true,
    "automerge": true,
    "ignoreTests": true,
    "registryUrls": [
      "https://papermc.io/repo/repository/maven-public/",
      "https://repo.maven.apache.org/maven2/",
      "https://eldonexus.de/repository/maven-releases/"
    ]
  },
  "gradle-wrapper": {
    "enabled": true,
    "automerge": true,
    "ignoreTests": true
  },
  "pipenv": {
    "enabled": true,
    "automerge": true
  },
  "dockerfile": {
    "enabled": true,
    "automerge": true
  }
}
